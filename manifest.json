{
  "manifest_version": 3,
  "name": "Document XLSX Downloader",
  "version": "2.0",
  "description": "Extract and export Google Sheets and Feishu documents as XLSX files",
  "permissions": [
    "activeTab",
    "scripting",
    "downloads",
    "storage",
    "tabs",
    "cookies",
    "webRequest",
    "declarativeNetRequest"
  ],
    "host_permissions": [
    "https://docs.google.com/spreadsheets/*",
    "https://open.feishu.cn/*",
    "https://*.feishu.cn/*",
    "http://localhost:3000/*",
    "https://web2csv-server.a1594834522.workers.dev/*"
      ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Download Document as CSV"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}